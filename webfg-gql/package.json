    "@aws-sdk/client-dynamodb": "^3.758.0",
    "@aws-sdk/lib-dynamodb": "^3.758.0",
    "uuid": "^11.1.0"
  },
  "config": {
    "qa_schema": "v119",
    "prod_schema": "v119",
    "stack_name": "webfg-gql"
