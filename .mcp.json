{
    "mcpServers": {
        "puppeteer": {
          "type": "stdio",
          "command": "npx",
	  "args": ["-y", "@modelcontextprotocol/server-puppeteer@2025.4.7"],
          "env": {}
        },
	"discord": {
  	  "type": "stdio",
  	  "command": "npx",
  	  "args": ["-y", "v-3/discordmcp"],
	  "env": { "DISCORD_TOKEN": "${env:DISCORD_BOT_TOKEN}" }
	},
        "context7": {
	  "type": "sse",
	  "url": "https://mcp.context7.com/sse"
	}
    }
}
